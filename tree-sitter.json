{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/config.schema.json",
  "grammars": [
    {
      "name": "gleam",
      "camelcase": "Gleam",
      "title": "Gleam",
      "scope": "source.gleam",
      "file-types": ["gleam"],
      "injection-regex": "^gleam$",
      "class-name": "TreeSitterGleam"
    }
  ],
  "metadata": {
    "version": "0.1.0",
    "license": "Apache 2.0",
    "description": "Gleam grammar for tree-sitter",
    "authors": [
      {
        "name": "Louis Pilfold",
        "email": "louis@lpil.uk",
        "url": "https://lpil.uk"
      },
      {
        "name": "Isaac Harris-Holt",
        "email": "isaac@ihh.dev",
        "url": "https://ihh.dev"
      }
    ],
    "links": {
      "repository": "https://github.com/rover-app/tree-sitter-gleam"
    }
  },
  "bindings": {
    "c": true,
    "go": true,
    "node": true,
    "python": true,
    "rust": true,
    "swift": true,
    "zig": false
  }
}
